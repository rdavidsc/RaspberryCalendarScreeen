{
  "name": "raspberry-event-screen",
  "productName": "raspberry-event-screen",
  "version": "0.1.0",
  "description": "Upcomming events screen",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Ram√≥n David Sifuentes",
    "email": "ramon@gobusinessinc.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/assets/img/LogoGBsq512x512.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "raspberry-event-screen"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "icon": "src/assets/img/LogoGBsq512x512.icns",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "icon": "src/assets/img/LogoGBsq512x512.png",
            "name": "raspberry-event-screen"
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "electron": "11.2.3",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "googleapis": "^39.2.0",
    "popper": "^1.0.1"
  }
}
